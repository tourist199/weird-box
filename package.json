    "react-markdown": "^6.0.2",
    "react-router-dom": "^5.2.0",
    "react-spring": "^8.0.27",
    "socket.io": "^4.1.2",
    "socket.io-client": "^4.1.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
