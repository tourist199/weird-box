  "dependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.1.5",
    "@parcel/transformer-image": "^2.0.0-nightly.2325",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "dotenv": "^8.6.0",
